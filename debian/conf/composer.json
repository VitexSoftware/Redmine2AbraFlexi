{
  "name": "deb/redmine2abraflexi",
  "description": "Reminder sender for AbraFlexi written in PHP",
  "type": "project",
  "authors": [
    {
      "name": "Vítězslav Dvořák",
      "email": "info@vitexsoftware.cz"
    }
  ],
  "config": {
    "vendor-dir": "/var/lib/composer/redmine2abraflexi"
  },
  "require": {
    "deb/ease-core": "*",
    "deb/abraflexi": "*"
  },
  "autoload": {
    "psr-4": {
      "Redmine2AbraFlexi\\": "./Redmine2AbraFlexi/"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "/usr/share/php/EaseCore"
    },
    {
      "type": "path",
      "url": "/usr/share/php/AbraFlexi"
    }
  ]
}
